@article{Botto:2017kh,
author = {Botto, Anna and Ganora, Daniele and Claps, Pierluigi and Laio, Francesco},
title = {{Technical note: Design flood under hydrological uncertainty}},
journal = {Hydrology and Earth System Sciences},
year = {2017},
volume = {21},
number = {7},
pages = {--3358},
annote = {There is some good stuff buried in here -- use of linear cost functions, dependence on length of sample record, etc},
publisher = {Copernicus GmbH},
doi = {10.5194/hess-21-3353-2017},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-04-22T16:17:55GMT},
date-modified = {2018-06-22T00:01:57GMT},
abstract = {Planning and verification of hydraulic infrastructures require a design estimate of hydrologic variables, usually provided by frequency analysis, and neglecting hydrologic uncertainty. However, when hydrologic uncertainty is accounted for, the design flood value for a specific return period is no longer a unique value, but is represented by a distribution of values. As a consequence, the design flood is no longer univocally defined, making the design process undetermined. <br><br> The Uncertainty Compliant Design Flood Estimation (UNCODE) procedure is a novel approach that, starting from a range of possible design flood estimates obtained in uncertain conditions, converges to a single design value. This is obtained through a cost{\textendash}benefit criterion with additional constraints that is numerically solved in a simulation framework. This paper contributes to promoting a practical use of the UNCODE procedure without resorting to numerical computation. A modified procedure is proposed by using a correction coefficient that modifies the standard (i.e., uncertainty-free) design value on the basis of sample length and return period only. The procedure is robust and parsimonious, as it does not require additional parameters with respect to the traditional uncertainty-free analysis. <br><br> Simple equations to compute the correction term are provided for a number of probability distributions commonly used to represent the flood frequency curve. The UNCODE procedure, when coupled with this simple correction factor, provides a robust way to manage the hydrologic uncertainty and to go beyond the use of traditional safety factors. With all the other parameters being equal, an increase in the sample length reduces the correction factor, and thus the construction costs, while still keeping the same safety level.},
url = {https://www.hydrol-earth-syst-sci.net/21/3353/2017/},
local-url = {file://localhost/Users/james/Google%20Drive/Apps/Papers/Library.papers3/Files/DD/DDA6D516-9E83-4698-AB8E-2861866D7D54.pdf},
file = {{DDA6D516-9E83-4698-AB8E-2861866D7D54.pdf:/Users/james/Google Drive/Apps/Papers/Library.papers3/Files/DD/DDA6D516-9E83-4698-AB8E-2861866D7D54.pdf:application/pdf;DDA6D516-9E83-4698-AB8E-2861866D7D54.pdf:/Users/james/Google Drive/Apps/Papers/Library.papers3/Files/DD/DDA6D516-9E83-4698-AB8E-2861866D7D54.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.5194/hess-21-3353-2017}}
}

