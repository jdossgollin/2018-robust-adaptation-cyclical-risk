@article{Rothlisberger2016,
author = {Rothlisberger, Matthias and Pfahl, Stephan and Martius, Olivia},
title = {{Regional-Scale Jet Waviness Modulates the Occurrence of Mid-Latitude Weather Extremes}},
journal = {Geophysical Research Letters},
year = {2016},
volume = {43},
number = {20},
pages = {2016GL070944--10,997},
month = jan,
doi = {10.1002/2016GL070944},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-04-22T16:18:21GMT},
date-modified = {2018-06-22T00:01:57GMT},
abstract = {Several studies have attributed the occurrence of recent weather extremes to an amplified waviness of the upper-tropospheric jet stream. Although trends in jet waviness are still under discussion, it is crucial to better understand the mechanisms through which jet waviness affects weather extremes. Here we show that variations in jet waviness on regional scales effectively modulate the occurrence of daily weather extremes, however, in regionally different ways. The jet waviness over the North Atlantic and the North Pacific mainly affects where wind, precipitation and cold extremes occur, while a wavy jet over Eurasia strongly favors the occurrence of hot extremes in summer. This is because regional variations of jet waviness are intrinsically linked to the occurrence and tracks of synoptic-scale weather systems, which can trigger the extremes. We conclude that potential jet waviness changes would affect the occurrence of weather extremes differently depending on where these changes occur.},
url = {http://doi.wiley.com/10.1002/2016GL070944},
local-url = {file://localhost/Users/james/Google%20Drive/Apps/Papers/Library.papers3/Files/EE/EE68300B-5182-4C27-BBC1-C4F7D48F1A59.pdf},
file = {{EE68300B-5182-4C27-BBC1-C4F7D48F1A59.pdf:/Users/james/Google Drive/Apps/Papers/Library.papers3/Files/EE/EE68300B-5182-4C27-BBC1-C4F7D48F1A59.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1002/2016GL070944}}
}

