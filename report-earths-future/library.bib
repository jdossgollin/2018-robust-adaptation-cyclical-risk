@article{Palmer:2013dx,
author = {Palmer, T N},
title = {{Climate extremes and the role of dynamics}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2013},
volume = {110},
number = {14},
pages = {5281--5282},
month = apr,
annote = {I quite like this paper, very straightforward},
publisher = {National Academy of Sciences},
affiliation = {Clarendon Laboratory, University of Oxford, Oxford OX1 3PU, United Kingdom. t.n.palmer@atm.ox.ac.uk},
doi = {10.1073/pnas.1303295110},
pmid = {23515330},
pmcid = {PMC3619291},
language = {English},
read = {Yes},
rating = {5},
date-added = {2018-04-22T16:17:42GMT},
date-modified = {2018-08-29T19:50:53GMT},
abstract = {One of the most societally important manifestations of climate change is the changing frequency and amplitude of extreme weather and climate events. A simple conceptual picture of why climate change may lead to an increase in weather extremes is that as the atmosphere warms, the specific humidity of the air increases, and in regions of enhanced latent heat release circulation patterns become more intense. Such arguments, although relevant, are primarily thermodynamic in nature; the effect on the dynamics of the climate system is secondary. However, those with a background in climate dynamics have little doubt that dynamical considerations will play as important a role in our understanding of climate change as the simpler thermodynamic arguments. Unfortunately, our understanding of the dynamical processes that influence such extremes is currently rather poor. The report by Petoukhov et al. (1), aims at understanding how recent extreme events such as the heat waves in Europe in 2003, Russia in 2010, and North America in 2011, provide a hint at the primacy of dynamics in understanding future changes in weather and climate extremes. The focus of Petoukhov et al.{\textquoteright}s (1) study is the midlatitude atmospheric Rossby wave (e.g., ref. 2). Such atmospheric Rossby waves can be thought of as describing planetary-scale oscillations in the latitude of the jet stream: the {\textquotedblleft}river{\textquotedblright} of fast moving air that circumnavigates the planet in the upper troposphere in the {\ldots} [↵][1]1E-mail: t.n.palmer{at}atm.ox.ac.uk. [1]: {\#}xref-corresp-1-1},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1303295110},
local-url = {file://localhost/Users/james/Google%20Drive/Apps/Papers/Library.papers3/Files/32/3261E8D0-D9DC-4FD0-A772-122DB2ACC4BB.pdf},
file = {{3261E8D0-D9DC-4FD0-A772-122DB2ACC4BB.pdf:/Users/james/Google Drive/Apps/Papers/Library.papers3/Files/32/3261E8D0-D9DC-4FD0-A772-122DB2ACC4BB.pdf:application/pdf;3261E8D0-D9DC-4FD0-A772-122DB2ACC4BB.pdf:/Users/james/Google Drive/Apps/Papers/Library.papers3/Files/32/3261E8D0-D9DC-4FD0-A772-122DB2ACC4BB.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1073/pnas.1303295110}}
}

@article{Rosner:2014cz,
author = {Rosner, Ana and Vogel, Richard M and Kirshen, Paul H},
title = {{A Risk-Based Approach to Flood Management Decisions in a Nonstationary World}},
journal = {Water Resources Research},
year = {2014},
volume = {50},
number = {3},
pages = {1928--1942},
month = mar,
doi = {10.1002/2013WR014561},
language = {English},
read = {Yes},
rating = {0},
date-added = {2018-04-22T16:18:10GMT},
date-modified = {2018-06-22T00:01:57GMT},
abstract = {Traditional approaches to flood management in a nonstationary world begin with a null hypothesis test of ``no trend'' and its likelihood, with little or no attention given to the likelihood that we might ignore a trend if it really existed. Concluding a trend exists when it does not, or rejecting a trend when it exists are known as type I and type II errors, respectively. Decision-makers are poorly served by statistical and/or decision methods that do not carefully consider both over- and under-preparation errors, respectively. Similarly, little attention is given to how to integrate uncertainty in our ability to detect trends into a flood management decision context. We show how trend hypothesis test results can be combined with an adaptation's infrastructure costs and damages avoided to provide a rational decision approach in a nonstationary world. The criterion of expected regret is shown to be a useful metric that integrates the statistical, economic, and hydrological aspects of the flood management problem in a nonstationary world.},
url = {http://doi.wiley.com/10.1002/2013WR014561},
local-url = {file://localhost/Users/james/Google%20Drive/Apps/Papers/Library.papers3/Files/EB/EB892A8B-8D24-4CCD-BE47-D2D3A59E14AB.pdf},
file = {{EB892A8B-8D24-4CCD-BE47-D2D3A59E14AB.pdf:/Users/james/Google Drive/Apps/Papers/Library.papers3/Files/EB/EB892A8B-8D24-4CCD-BE47-D2D3A59E14AB.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1002/2013WR014561}}
}

