@article{ding:2005,
  title = {Circumglobal {{Teleconnection}} in the {{Northern Hemisphere Summer}}},
  volume = {18},
  issn = {0894-8755},
  doi = {10.1175/JCLI3473.1},
  abstract = {Analysis of the 56-yr NCEP\textendash{}NCAR reanalysis data reveals a recurrent circumglobal teleconnection (CGT) pattern in the summertime midlatitude circulation of the Northern Hemisphere. This pattern represents the second leading empirical orthogonal function of interannual variability of the upper-tropospheric circulation. The CGT, having a zonal wavenumber-5 structure, is primarily positioned within a waveguide that is associated with the westerly jet stream. The spatial phases of CGT tend to lock to preferred longitudes. The geographically phase-locked patterns bear close similarity during June, August, and September, but the pattern in July shows shorter wavelengths in the North Pacific\textendash{}North America sector. The CGT is accompanied by significant rainfall and surface air temperature anomalies in the continental regions of western Europe, European Russia, India, east Asia, and North America. This implies that the CGT may be a source of climate variability and predictability in the above-mentioned midlatitude regions. The CGT has significant correlations with the Indian summer monsoon (ISM) and El Ni\~no\textendash{}Southern Oscillation (ENSO). However, in normal ISM years the CGT\textendash{}ENSO correlation disappears; on the other hand, in the absence of El Ni\~no or La Ni\~na, the CGT\textendash{}ISM correlation remains significant. It is suggested that the ISM acts as a ``conductor'' connecting the CGT and ENSO. When the interaction between the ISM and ENSO is active, ENSO may influence northern China via the ISM and the CGT. Additionally, the variability of the CGT has no significant association with the Arctic Oscillation and the variability of the western North Pacific summer monsoon. The circulation of the wave train shows a barotropic structure everywhere except the cell located to the northwest of India, where a baroclinic circulation structure dominates. Two possible scenarios are proposed. The abnormal ISM may excite an anomalous west-central Asian high and downstream Rossby wave train extending to the North Pacific and North America. On the other hand, a wave train that is excited in the jet exit region of the North Atlantic may affect the west-central Asian high and, thus, the intensity of the ISM. It is hypothesized that the interaction between the global wave train and the ISM heat source may be instrumental in maintaining the boreal summer CGT.},
  number = {17},
  journal = {J. Climate},
  author = {Ding, Qinghua and Wang, Bin},
  month = sep,
  year = {2005},
  pages = {3483-3505},
  file = {/Users/james/Zotero/storage/USFGDGK7/Ding and Wang - 2005 - Circumglobal Teleconnection in the Northern Hemisp.pdf;/Users/james/Zotero/storage/XCRPEZGI/JCLI3473.html}
}

