  file = {/Users/james/Zotero/storage/F4NTMB9Z/AF62047C-DC1A-4BE1-9DB9-CA2F3B4102F5.pdf;/Users/james/Zotero/storage/TFPGDRX6/AF62047C-DC1A-4BE1-9DB9-CA2F3B4102F5.pdf},
