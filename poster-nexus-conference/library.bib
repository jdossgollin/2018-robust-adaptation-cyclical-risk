  pages = {1-28},
  file = {/Users/james/Zotero/storage/GFTV9FEH/A3059BC6-D97B-4A0A-9620-EF0398930CAD.pdf;/Users/james/Zotero/storage/XSV29B39/A3059BC6-D97B-4A0A-9620-EF0398930CAD.pdf},
  publisher = {{Copernicus GmbH}},
  read = {Yes},
